<launch>

  <arg name="uav_name" default="$(optenv UAV_NAME uav)"/>

  <arg name="debug" default="false" />
  <arg unless="$(arg debug)" name="launch_prefix" value=""/>
  <arg     if="$(arg debug)" name="launch_prefix" value="pdb"/>

  <node name="localization_evaluation" pkg="balloon_planner" type="csv_eval.py" output="screen" launch-prefix="$(arg launch_prefix)">
    <param name="out_fname" value="$(find balloon_planner)/output/loc_out.csv" />
  </node>

</launch>
